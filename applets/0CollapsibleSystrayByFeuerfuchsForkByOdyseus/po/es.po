msgstr "Ícono de expansión horizontal:"

#. settings-schema.json->pref_horizontal_expand_icon_name->tooltip
#. settings-schema.json->pref_horizontal_collapse_icon_name->tooltip
msgid "Enter either the icon's qualified name or it's full path"
msgstr "Ingresar el nombre calificado del ícono o su ruta completa."

#. settings-schema.json->pref_horizontal_collapse_icon_name->description
msgid "Horizontal collapse icon: "
msgstr "Ícono de colapsamiento horizontal:"

